  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-rabbitmq",
    "rabbitmq"
  ],
  "dependencies": {
    "amqplib": "^0.5.5",
    "bluebird": "^3.5.5"
  },
  "devDependencies": {
    "autod": "^3.0.1",
